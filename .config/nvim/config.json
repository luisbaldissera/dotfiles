{
  "ui": {
    "colorscheme": "gruvbox",
    "filetype": "plugin indent on",
    "syntax": "on"
  },
  "options": {
    "autoindent": true,
    "background": "dark",
    "backup": false,
    "cursorline": true,
    "expandtab": true,
    "hlsearch": true,
    "ignorecase": true,
    "incsearch": true,
    "magic": true,
    "number": true,
    "relativenumber": true,
    "shiftwidth": {
      "global": 2,
      "local": {
        "java": 4,
        "bash": 4,
        "sh": 4
      }
    },
    "shortmess": "filmrnxtToOF",
    "sidescroll": 5,
    "signcolumn": "yes",
    "smartcase": true,
    "smartindent": true,
    "termguicolors": true,
    "updatetime": 300,
    "writebackup": false,
    "foldmethod": {
      "global": "syntax",
      "local": {
        "vim": "marker"
      }
    },
    "foldlevel": {
      "global": 999,
      "local": {
        "sql": 0,
        "vim": 0
      }
    },
    "tabstop": {
      "global": "2",
      "local": {
        "java": 4,
        "bash": 4,
        "sh": 4
      }
    }
  },
  "plugins": {
    "Joorem/vim-haproxy": {},
    "Yggdroot/indentLine": {
      "options": {
        "g:indentLine_char": "|",
        "g:indentLine_fileTypeExclude": [
          "",
          "coc-explorer",
          "help",
          "man",
          "plaintext",
          "terminal"
        ]
      }
    },
    "airblade/vim-gitgutter": {},
    "ap/vim-css-color": {},
    "fannheyward/telescope-coc.nvim": {},
    "fladson/vim-kitty": {},
    "honza/vim-snippets": {},
    "itchyny/lightline.vim": {
      "options": {
        "g:lightline": {
          "colorscheme": "powerline",
          "active": {
            "left": [
              ["mode", "paste"],
              ["cocstatus", "gitbranch", "readonly", "filename", "modified"]
            ]
          },
          "component_function": {
            "gitbranch": "FugitiveHead",
            "cocstatus": "coc#status"
          }
        }
      }
    },
    "junegunn/vim-peekaboo": {},
    "junkblocker/git-time-lapse": {},
    "kshenoy/vim-signature": {},
    "leafgarland/typescript-vim": {},
    "lervag/vimtex": {},
    "liuchengxu/space-vim-theme": {},
    "maxmellon/vim-jsx-pretty": {},
    "morhetz/gruvbox": {
      "options": {
        "g:gruvbox_contrast_dark": "hard"
      }
    },
    "neoclide/coc.nvim": {
      "install": {
        "branch": "release"
      },
      "options": {
        "g:coc_snippet_next": "<TAB>",
        "g:coc_global_extensions": [
          "coc-docker",
          "coc-explorer",
          "coc-format-json",
          "coc-java",
          "coc-java-debug",
          "coc-json",
          "coc-lua",
          "coc-markdown-preview-enhanced",
          "coc-markdownlint",
          "coc-marketplace",
          "coc-prettier",
          "coc-snippets",
          "coc-solargraph",
          "coc-sql",
          "coc-tsserver",
          "coc-vimlsp",
          "coc-vimtex",
          "coc-yaml"
        ]
      }
    },
    "nvim-lua/plenary.nvim": {},
    "nvim-lua/popup.nvim": {},
    "nvim-telescope/telescope-file-browser.nvim": {},
    "nvim-telescope/telescope-github.nvim": {},
    "nvim-telescope/telescope.nvim": {},
    "nvim-treesitter/nvim-treesitter": {
      "install": {
        "do": ":TSUpdate"
      }
    },
    "pangloss/vim-javascript": {},
    "peitalin/vim-jsx-typescript": {},
    "puremourning/vimspector": {
      "options": {
        "g:vimspector_install_gadgets": [
          "vscode-cpptools",
          "vscode-node-debug2"
        ]
      }
    },
    "rhysd/git-messenger.vim": {},
    "sonph/onehalf": {
      "install": {
        "rtp": "vim"
      }
    },
    "stevearc/vim-arduino": {},
    "tkhren/vim-fake": {},
    "tomasr/molokai": {},
    "tpope/vim-commentary": {},
    "tpope/vim-fugitive": {},
    "tpope/vim-liquid": {},
    "tpope/vim-rails": {},
    "tpope/vim-sensible": {},
    "tpope/vim-surround": {},
    "vim-ruby/vim-ruby": {}
  }
}
